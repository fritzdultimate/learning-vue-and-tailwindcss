{
  "name": "learning-vue-and-tailwindcss",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@headlessui/vue": "^1.7.2",
    "@heroicons/vue": "^2.0.11",
    "axios": "^0.27.2",
    "esbuild": "^0.14.43",
    "mdue": "^0.1.4",
    "moralis": "^1.8.0",
    "numeral": "^2.0.6",
    "pinia": "^2.0.13",
    "qrcode.vue": "^3.3.3",
    "swiper": "^8.1.5",
    "topbar": "^1.0.1",
    "vue": "^3.2.33",
    "vue-agile": "^2.0.0",
    "vue-awesome": "^4.5.0",
    "vue-debounce": "^4.0.0",
    "vue-router": "^4.0.14",
    "vue-slick-carousel": "^1.0.6",
    "vue-unicons": "^3.3.1",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.5",
    "cypress": "^9.5.4",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7"
  },
  "description": "Repo where I create project while learning Vue + Tailwindcss",
  "main": "postcss.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fritzdultimate/learning-vue-and-tailwindcss.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fritzdultimate/learning-vue-and-tailwindcss/issues"
  },
  "homepage": "https://github.com/fritzdultimate/learning-vue-and-tailwindcss#readme",
  "engines": {
    "node": "16.14.0"
  }
}
